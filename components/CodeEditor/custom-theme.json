{
  "base": "vs",
  "inherit": true,
  "rules": [
    { "token": "", "foreground": "333333", "background": "ffffff" },
    { "token": "invalid", "foreground": "ff552e" },
    { "token": "emphasis", "fontStyle": "italic" },
    { "token": "strong", "fontStyle": "bold" },

    { "token": "variable", "foreground": "009fd4" },
    { "token": "variable.predefined", "foreground": "009fd4" },
    { "token": "constant", "foreground": "ff552e" },
    { "token": "constant.language.boolean", "foreground": "c67cb0" },
    { "token": "comment", "foreground": "bdbdbd" },
    { "token": "number", "foreground": "c67cb0" },
    { "token": "number.hex", "foreground": "c67cb0" },
    { "token": "regexp", "foreground": "c67cb0" },
    { "token": "annotation", "foreground": "808080" },
    { "token": "type", "foreground": "009fd4" },

    { "token": "delimiter", "foreground": "000000" },

    { "token": "tag", "foreground": "ff552e" },
    { "token": "metatag", "foreground": "ff552e" },

    { "token": "key", "foreground": "db401d" },

    { "token": "attribute.name", "foreground": "ff552e" },
    { "token": "attribute.value", "foreground": "009fd4" },
    { "token": "attribute.value.number", "foreground": "fb96b4" },

    { "token": "string", "foreground": "ff552e" },
    { "token": "keyword", "foreground": "009fd4" },
    { "token": "keyword.operator", "foreground": "c67cb0" },
    { "token": "keyword.control", "foreground": "c67cb0" },
    { "token": "support", "foreground": "c67cb0" },
    { "token": "storage", "foreground": "c67cb0" },
    { "token": "punctuation", "foreground": "c67cb0" }
  ],
  "colors": {
    "editor.foreground": "#000000",
    "editor.background": "#FFFFFF",
    "editor.selectionBackground": "#eff9fd",
    "editor.lineHighlightBackground": "#f9f9f9",
    "editorCursor.foreground": "#00ff88"
  }
}
